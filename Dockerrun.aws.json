{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "server",
      "image": "magicnc7/medium-project",
      "portMappings": [
        {
          "HostPort": "3000",
          "ContainerPort": "3000"
        }
      ],
      "links": ["db", "redis"]
    },
    {
      "name": "db",
      "image": "postgres:15",
      "portMappings": [
        {
          "HostPort": "5432",
          "ContainerPort": "5432"
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_DB",
          "value": "test"
        },
        {
          "name": "POSTGRES_USER",
          "value": "test"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "test"
        },
        {
          "name": "POSTGRES_INITDB_ARGS",
          "value": "--encoding=UTF-8"
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "portMappings": [
        {
          "HostPort": "6379",
          "ContainerPort": "6379"
        }
      ]
    }
  ]
}
