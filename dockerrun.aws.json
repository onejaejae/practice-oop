{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "server",
      "image": {
        "Name": "magicnc7/test-project",
        "Update": "true"
      },
      "portMappings": [
        {
          "hostPort": "3000",
          "containerPort": "3000"
        }
      ],
      "links": ["db", "redis"]
    },
    {
      "name": "db",
      "image": "postgres:15",
      "portMappings": [
        {
          "hostPort": "5432",
          "containerPort": "5432"
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_DB",
          "value": "test"
        },
        {
          "name": "POSTGRES_USER",
          "value": "test"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "test"
        },
        {
          "name": "POSTGRES_INITDB_ARGS",
          "value": "--encoding=UTF-8"
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "portMappings": [
        {
          "hostPort": "6379",
          "containerPort": "6379"
        }
      ]
    }
  ]
}
